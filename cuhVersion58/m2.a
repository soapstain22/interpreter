          .extern i
          .extern j
          .global sub
sub:      push lr
          push fp
          mov fp, sp

          ld r0, i
          ld r1, x
          ldr r1, r1, 0
          add r0, r0, r1
          ld r1, y
          ldr r1, r1, 0
          add r0, r0, r1
          dout r0
          nl

          mov sp, fp
          pop fp
          pop lr
          ret
x:        .word j      ; .word equivalent to .fill
y:        .word z
z:        .word 11