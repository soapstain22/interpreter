          .extern i
          .extern j
          .global sub
sub:      halt
          push lr
          push fp
          mov fp, sp

          ld r0, j-1
          ld r1, x
          ldr r1, r1, 0
          add r0, r0, r1
          ld r1, z
          ldr r1, r1, 0
          add r0, r0, r1
          dout r0
          nl

          mov sp, fp
          pop fp
          pop lr
          ret
x:        .word z-1    
c:        .word 11
z:        .word i+2
